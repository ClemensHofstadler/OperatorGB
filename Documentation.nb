       SuperscriptBox["b", "-"], "**", "b"}], "-", "b"}], ",", 
     RowBox[{
      RowBox[{
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"], "**", 
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"]}], "-", 
      RowBox[{
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"]}]}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Q", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "V", ",", "W"}], "}"}], ",", 
