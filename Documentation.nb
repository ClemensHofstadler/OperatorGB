     SuperscriptBox["b", "-"], "**", "b", "**", 
     SuperscriptBox["b", "-"]}], "+", " ", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"QSignature", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"f", ",", "g"}], "}"}], ",", "Q"}], "]"}]}], "Input",
