 RowBox[{
  RowBox[{"claim", "=", " ", 
   RowBox[{
    RowBox[{"a", "**", "b", "**", 
     SuperscriptBox["b", "-"], "**", 
     SuperscriptBox["a", "-"], "**", "a", "**", "b"}], "-", 
    RowBox[{"a", "**", "b"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.747320340521337*^9, 3.74732047320422*^9}, {
