 RowBox[{
  RowBox[{"assumptions", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "**", 
       SuperscriptBox["a", "-"], "**", "a"}], "-", "a"}], ",", 
     RowBox[{
      RowBox[{"b", "**", 
       SuperscriptBox["b", "-"], "**", "b"}], "-", "b"}], ",", 
     RowBox[{
      RowBox[{
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"], "**", 
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"]}], "-", 
      RowBox[{
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"]}]}]}], "}"}]}], ";"}]], "Input",
