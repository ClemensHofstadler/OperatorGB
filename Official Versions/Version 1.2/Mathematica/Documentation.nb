   3.775806491912137*^9, 3.777661248355698*^9, 3.777723225943563*^9, 
   3.780073263689253*^9, {3.781955824592572*^9, 3.781955842631633*^9}, {
   3.781955878638941*^9, 3.78195592349886*^9}, {3.781956373320661*^9, 
   3.781956430777342*^9}, {3.781956476362343*^9, 3.781956516341372*^9}, {
   3.7819568195454187`*^9, 3.781956942525235*^9}, {3.781957455745454*^9, 
   3.7819574593364058`*^9}, {3.7824685041563683`*^9, 3.782468543715335*^9}, {
