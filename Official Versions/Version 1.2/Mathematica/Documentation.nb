 RowBox[{
  RowBox[{"Q", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "V", ",", "W"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       SuperscriptBox["a", "-"], ",", "W", ",", "V"}], "}"}], ",", 
     RowBox[{"{", 
