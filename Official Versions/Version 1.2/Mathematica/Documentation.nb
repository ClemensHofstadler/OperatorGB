    RowBox[{"a", ",", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
        SuperscriptBox["b", "-"]}]}], "+", 
      RowBox[{
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"], "**", 
       SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
       SuperscriptBox["b", "-"]}]}], ",", "b"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"-", "1"}], ",", 
     RowBox[{
      RowBox[{"-", "a"}], "+", 
      RowBox[{"a", "**", 
       SuperscriptBox["a", "-"], "**", "a"}]}], ",", 
     RowBox[{"b", "**", 
      SuperscriptBox["b", "-"], "**", 
      SuperscriptBox["a", "-"], "**", "a", "**", "b", "**", 
      SuperscriptBox["b", "-"], "**", "b"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"1", ",", 
     RowBox[{
      RowBox[{"-", "a"}], "+", 
      RowBox[{"a", "**", 
       SuperscriptBox["a", "-"], "**", "a"}]}], ",", 
     RowBox[{"b", "**", 
      SuperscriptBox["b", "-"], "**", "b"}]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.7666732133480253`*^9, 3.766673232670034*^9}, {
   3.7746294686929493`*^9, 3.774629491193653*^9}, {3.774629975446145*^9, 
