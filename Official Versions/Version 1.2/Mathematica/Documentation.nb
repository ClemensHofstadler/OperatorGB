   3.782469508068233*^9, 3.782472349929427*^9, 3.782472841826635*^9, {
   3.7909187639613123`*^9, 3.790918792459051*^9}, 3.790918866830159*^9, 
   3.790918914844441*^9, 3.790921247774625*^9},
