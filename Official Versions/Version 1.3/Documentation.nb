Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Beta]", "**", "f"}], "-", 
    RowBox[{
     SuperscriptBox["f", "\[Prime]",
      MultilineFunction->None], "**", "\[Alpha]"}]}], ",", 
   RowBox[{
    RowBox[{"\[Gamma]", "**", "g"}], "-", 
    RowBox[{
     SuperscriptBox["g", "\[Prime]",
      MultilineFunction->None], "**", "\[Beta]"}]}], ",", 
   RowBox[{
    RowBox[{"\[Delta]", "**", "h"}], "-", 
    RowBox[{
     SuperscriptBox["h", "\[Prime]",
      MultilineFunction->None], "**", "\[Gamma]"}]}], ",", 
   RowBox[{"g", "**", "f"}], ",", 
