  RowBox[{"{", 
   RowBox[{
    SubscriptBox["e", "1"], ",", 
    SubscriptBox["e", "2"], ",", 
    SubscriptBox["e", "3"]}], "}"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"SetUpRing", "[", 
  RowBox[{"{", 
   RowBox[{"x", ",", "y", ",", "z"}], "}"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"y", "**", "x", "**", 
       SubscriptBox["e", "2"], "**", "y"}], "-", 
      SubscriptBox["e", "2"]}], ",", 
     RowBox[{
      RowBox[{
       SubscriptBox["e", "1"], "**", "y", "**", "y"}], "+", 
      RowBox[{"x", "**", 
       SubscriptBox["e", "2"]}]}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"G", " ", "=", " ", 
  RowBox[{"ModuleGroebner", "[", 
   RowBox[{"cofactors", ",", "M", ",", "3"}], "]"}]}]}], "Input",
