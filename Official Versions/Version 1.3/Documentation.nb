   3.777661175347465*^9, 3.777661216806656*^9}, {3.781426165324749*^9, 
   3.781426424584104*^9}, {3.781955794002886*^9, 3.7819557954884377`*^9}, {
   3.781957474392661*^9, 3.78195748589213*^9}, {3.781982713536878*^9, 
   3.781982721701476*^9}, 3.782469234217931*^9, {3.782472341898717*^9, 
   3.78247234830667*^9}},ExpressionUUID->"f3dd2d66-2f73-4b39-a60f-\
5155b992c2f7"],

