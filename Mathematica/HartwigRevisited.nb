    RowBox[{"R", "(", 
     RowBox[{
      SuperscriptBox["A", "*"], "A", " ", "P"}], ")"}], "\[SupersetEqual]", 
    RowBox[{"R", "(", 
     SuperscriptBox["Q", "*"], ")"}]}], TraditionalForm]],ExpressionUUID->
  "677b5741-31f5-4cc5-8f52-3ca31aaa0552"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", 
     RowBox[{"C", " ", 
      SuperscriptBox["C", "*"], 
      SuperscriptBox["P", "*"]}], ")"}], "\[SubsetEqual]", 
